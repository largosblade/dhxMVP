{
  "manifest_version": 2,
  "name": "dhxMVP app demo - Extension version",
  "description": "Chrome Extension to provide quick access to dhMVP demo app",
  "version": "1.0",
  "browser_action": {
   "default_icon": "icon-16.png",
   "default_popup": "index.html",
   "default_title": "dhxMVP demo app"
  },
  "icons": { "16": "icon-16.png", "128": "icon-128.png" },
  "permissions": [
   "activeTab",
   "tabs", 
    "http://*/*", 
    "https://*/*"
   ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}